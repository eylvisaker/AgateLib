<Project name="TgzProviderTester" path="../Tests/Core/TgzProvider" language="C#" type="WinExe" frameworkVersion="v3_5">
  <Configuration name="Debug">
    <Options>
      <OutputPath>../../../Binaries/Debug/Tests</OutputPath>
    </Options>
  </Configuration>

  <Configuration name="Release">
    <Options>
      <OutputPath>../../../Binaries/Release/Tests</OutputPath>
    </Options>
  </Configuration>

  <Reference name="AgateLib"/>
  <Reference name="System"/>
  <Reference name="System.Core"/>
  <Reference name="System.Xml"/>

  <Files>
    <Match path="." pattern="*.cs" recurse="true"/>
    <Match path="./archives" pattern="*.tar.gz" buildAction="None" copyToOutput="Always" />
  </Files>
</Project>
